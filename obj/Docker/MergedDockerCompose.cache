services:
  sic43s1_aspnetcore_server:
    build:
      context: G:\SIC work\sic43s1-server-netcore
      dockerfile: sic43s1_aspnetcore_server\Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: sic43s1aspnetcoreserver
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\Windows 10\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\Windows 10\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
version: '3.4'